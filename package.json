		"test:unit": "vitest",
		"sst:deploy": "sst deploy",
		"sst:dev": "sst dev"
	},
	"devDependencies": {
		"@melt-ui/pp": "^0.1.2",
		"@melt-ui/svelte": "^0.55.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@types/uuid": "^9.0.6",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"aws-cdk-lib": "2.101.1",
		"constructs": "10.2.69",
		"eslint": "^8.28.0",
